{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@tsconfig/svelte/tsconfig.json",
    "compilerOptions": {
      "composite": true,
      "skipLibCheck": true,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "target": "ESNext",
      "useDefineForClassFields": true,
      "resolveJsonModule": true,
      "allowJs": true,
      "checkJs": true,
      "isolatedModules": true
    },
    "include": ["vite.config.ts"]
  }
  