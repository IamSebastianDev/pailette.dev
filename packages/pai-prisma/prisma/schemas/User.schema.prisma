import { RefreshToken } from "RefreshToken.schema"

model User {
    id              String          @id @unique @default(uuid())
    email           String          @unique
    name            String          @unique
    password        String
    oAiPIToken      String?          
    refreshTokens   RefreshToken[]
    createdAt       DateTime        @default(now())
    updatedAt       DateTime        @updatedAt
}